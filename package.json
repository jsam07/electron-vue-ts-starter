{
  "name": "electron-vue-ts-starter",
  "version": "1.0.0",
  "description": "Production Ready Electron-TypeScript Starter, built with Vue3 and Vite.",
  "main": "dist/main/index.cjs",
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "build": "vue-tsc --project src/renderer/tsconfig.json --noEmit && node scripts/build.mjs && electron-builder",
    "release:linux": "vue-tsc --project src/renderer/tsconfig.json --noEmit && node scripts/build.mjs && electron-builder build --linux",
    "release:mac": "vue-tsc --project src/renderer/tsconfig.json --noEmit && node scripts/build.mjs && electron-builder build --mac",
    "release:win": "vue-tsc --project src/renderer/tsconfig.json --noEmit && node scripts/build.mjs && electron-builder build --w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "cross-env NODE_ENV=test jest -i --colors --verbose --coverage --passWithNoTests",
    "test:watch": "npm run test -- --watchAll"
  },
  "keywords": [
    "electron",
    "typescript",
    "template",
    "starter",
    "boilerplate"
  ],
  "author": "Jamie Samuel",
  "license": "MIT",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/eslint": "^8.4.3",
    "@types/jest": "^28.1.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^10.3.0",
    "electron": "19.0.6",
    "electron-builder": "^23.1.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "eslint-plugin-vue": "^9.1.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-resolve": "^2.1.2",
    "vitest": "^0.16.0",
    "vue-tsc": "^0.38.2"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "jest": "^28.1.1",
    "pinia": "^2.0.14",
    "ts-jest": "^28.0.5",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "env": {
    "//": "Used in build scripts",
    "PORT": 3344
  }
}
